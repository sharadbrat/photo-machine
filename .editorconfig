# EditorConfig: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true

# Set default charset
[*.{js,css,html}]
charset = utf-8

# 2 space indentation
[*]
indent_style = space
indent_size = 2
